{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "azt-datafactory"
		},
		"aztdatalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'aztdatalake'"
		},
		"lssqldatab_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'lssqldatab'"
		},
		"aztdatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dafistorae.dfs.core.windows.net/"
		},
		"lsrestapi_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://pokeapi.co/api/v2/pokemon"
		},
		"lssqldatab_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "datserverdfro.database.windows.net"
		},
		"lssqldatab_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "sqldnsazure"
		},
		"lssqldatab_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "samueladmin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "azt-datafactory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"aztdatalake_accountKey": {
						"value": "[parameters('aztdatalake_accountKey')]"
					},
					"lssqldatab_password": {
						"value": "[parameters('lssqldatab_password')]"
					},
					"aztdatalake_properties_typeProperties_url": {
						"value": "[parameters('aztdatalake_properties_typeProperties_url')]"
					},
					"lsrestapi_properties_typeProperties_url": {
						"value": "[parameters('lsrestapi_properties_typeProperties_url')]"
					},
					"lssqldatab_properties_typeProperties_server": {
						"value": "[parameters('lssqldatab_properties_typeProperties_server')]"
					},
					"lssqldatab_properties_typeProperties_database": {
						"value": "[parameters('lssqldatab_properties_typeProperties_database')]"
					},
					"lssqldatab_properties_typeProperties_userName": {
						"value": "[parameters('lssqldatab_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}